{
  "manifest_version": 3,
  "name": "Google Meet Time Left",
  "version": "1.0",
  "description": "A Chrome extension that runs on Google Meet and shows time left in the meeting",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
